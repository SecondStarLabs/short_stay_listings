<p id="notice"><%= notice %></p>

<h1>Listing Quote Requests</h1>

<table>
  <thead>
    <tr>
      <th>Full name</th>
      <th>Email</th>
      <th>Monthly rent</th>
      <th>Beds</th>
      <th>Baths</th>
      <th>Start date</th>
      <th>End date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quote_requests.each do |quote_request| %>
      <tr>
        <td><%= quote_request.full_name %></td>
        <td><%= quote_request.email_address %></td>
        <td><%= quote_request.monthly_rent %></td>
        <td><%= quote_request.beds %></td>
        <td><%= quote_request.baths %></td>
        <td><%= quote_request.start_date %></td>
        <td><%= quote_request.end_date %></td>
        <td><%= link_to 'Show', quote_request %></td>
        <td><%= link_to 'Edit', edit_quote_request_path(quote_request) %></td>
        <td><%= link_to 'Destroy', quote_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Quote request', new_quote_request_path %>
