<form action="http://demo.favethemes.com/wp/realto/property/" method="get">
    <input name="post_type" value="property" type="hidden">
        
        <div class="row-fluid">
            <div class="span5">
                <input class="input-block-level" name="search_keyword" value="" placeholder="Full Name" type="text">
            </div>
            <div class="span4">
                <input class="input-block-level" name="search_keyword" value="" placeholder="Email" type="text">
            </div>
            <div class="span2 hidden-phone">
                <input class="btn input-block-level btn-realto-form" value="Request Quote" type="submit">
            </div>
        </div>
                
       
        <div class="row-fluid">
           
            <div class="span3">
                <select class="input-block-level select" name="property_type">
                    <option value="">Monthly Rent</option>
                    <option value="Unsure - Leave Notes">Unsure - Leave Notes</option>
                    <option value="$1800 to $2399">$1800 to $2399</option>
                    <option value="$2400 to $2899">$2400 to $2899</option>
                    <option value="$2900 to $3499">$2900 to $3499</option>
                    <option value="$3500 to $3999">$3500 to $3999</option>
                    <option value="$4000 to $4499">$4000 to $4499</option>
                    <option value="$4500 to $4999">$4500 to $4999</option>
                </select>
            </div>
                    
                  
            <div class="span2">
                <select class="input-block-level select" name="beds">
                    <option value="">Beds</option>
                    <option value="1">1+ Beds</option>
                    <option value="2">2+ Beds</option>
                    <option value="3">3+ Beds</option>
                    <option value="4">4+ Beds</option>
                    <option value="5">5+ Beds</option>
                    <option value="6">6+ Beds</option>
                </select>
            </div>
            
           
                       <div class="span2">
                <select class="input-block-level select" name="baths">
                    <option value="">Baths</option>
                    <option value="1">1+ Baths</option>
                    <option value="2">2+ Baths</option>
                    <option value="3">3+ Baths</option>
                    <option value="4">4+ Baths</option>
                    <option value="5">5+ Baths</option>
                    <option value="6">6+ Baths</option>
                </select>
            </div>
            
           
            <div class="span2">
                <input class="input-block-level select" name="min-price" value="" placeholder="Start Date" type="text">
            </div>
                      
            
            <div class="span2">
                <input class="input-block-level select" name="max-price" value="" placeholder="End Date" type="text">
            </div>
                        
            <div class="row-fluid visible-phone">
                <div class="span2">
                    <input class="btn input-block-level btn-realto-form" value="Search" type="submit">
                </div>
            </div>
            
        </div>
            				
    </form>
